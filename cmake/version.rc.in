#include <winver.h>

#ifndef VS_FF_DEBUG
#define VS_FF_DEBUG 0x1L
#endif

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,0
 PRODUCTVERSION @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,0
 FILEFLAGSMASK 0x3fL
#ifdef NDEBUG
 FILEFLAGS 0x0L
#else
 FILEFLAGS VS_FF_DEBUG
#endif
 FILEOS 0x40004L
 FILETYPE 0x2L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904B0" // Lang: U.S. English, Unicode
        BEGIN
            VALUE "CompanyName",      "@PROJECT_VENDOR@"
            VALUE "FileDescription",  "@PROJECT_DESCRIPTION@"
            VALUE "FileVersion",      "@PROJECT_VERSION@"
            VALUE "InternalName",     "@PROJECT_NAME@"
            VALUE "OriginalFilename", "@PROJECT_NAME@.dll"
            VALUE "ProductName",      "@PROJECT_DISPLAY_NAME@"
            VALUE "ProductVersion",   "@PROJECT_VERSION@"
            VALUE "LegalCopyright",   "@PROJECT_COPYRIGHT@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 0x04B0
    END
END